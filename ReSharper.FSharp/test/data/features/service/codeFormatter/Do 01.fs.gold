KeepExistingLineBreakInDoLikeExpr = True
  unaffected by DoLikeExprOnTheSameLine
do
  do printfn ""
  do ()

  do
    printfn ""
    printfn ""

  do // foo
    printfn ""
    printfn ""

  do printfn ""
     printfn ""

  ()

================
KeepExistingLineBreakInDoLikeExpr = False
  DoLikeExprOnTheSameLine = NEVER
do
  do
    printfn ""
  do
    ()

  do
    printfn ""
    printfn ""

  do // foo
    printfn ""
    printfn ""

  do
    printfn ""
    printfn ""

  ()

================
  DoLikeExprOnTheSameLine = IF_OWNER_IS_SINGLE_LINE
do
  do printfn ""
  do ()

  do
    printfn ""
    printfn ""

  do // foo
    printfn ""
    printfn ""

  do
    printfn ""
    printfn ""

  ()

================
  DoLikeExprOnTheSameLine = ALWAYS
do do printfn ""
   do ()

   do printfn ""
      printfn ""

   do // foo
     printfn ""
     printfn ""

   do printfn ""
      printfn ""

   ()
